<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <StackPane fx:id="flashcardListPanel">
    <VBox>
      <StackPane fx:id="flashcardHeaderBarPlaceholder" VBox.vgrow="NEVER" styleClass="pane-with-border" />
      <VBox>
        <ListView fx:id="flashcardListView" VBox.vgrow="ALWAYS" />
      </VBox>
    </VBox>
  </StackPane>
</VBox>
