@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":FlashCardAppParser" as FlashCardAppParser LOGIC_COLOR
participant ":ListCommand" as ListCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "model:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("list 4")
activate LogicManager

LogicManager -> FlashCardAppParser : parseCommand("list 4")
activate FlashCardAppParser

create ListCommand
FlashCardAppParser -> ListCommand
activate ListCommand

ListCommand --> FlashCardAppParser : listCommand(4)
deactivate ListCommand

FlashCardAppParser --> LogicManager : listCommand(4)
deactivate FlashCardAppParser

LogicManager -> ListCommand : execute()
activate ListCommand

ListCommand -> Model : updateFilteredFlashcardList()
activate Model

Model --> ListCommand
deactivate Model

ListCommand -> CommandResult
activate CommandResult

CommandResult --> ListCommand
deactivate CommandResult

ListCommand --> LogicManager : result
deactivate ListCommand

[<--LogicManager
deactivate LogicManager

@enduml